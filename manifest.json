{
  "manifest_version": 3,
  "name": "Nudelink",
  "version": "0.3.0",
  "action": {
    "default_popup": "popup.html",
    "default_title": "Nudelink"
  },
  "icons": {
    "16": "icons/icon-16.png",
    "32": "icons/icon-32.png",
    "128": "icons/icon-192.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ]
}